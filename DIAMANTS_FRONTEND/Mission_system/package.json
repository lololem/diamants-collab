{
  "name": "diamants-mission-system-v1",
  "version": "1.0.0",
  "type": "module",
  "description": "DIAMANTS Mission System with EZ-Tree Integration",
  "main": "index.html",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev:node16:cn": "./scripts/start-console-ninja.sh",
    "debug": "node scripts/debug-server.js",
    "console-ninja": "node scripts/debug-server.js",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview",
    "legacy:start": "live-server --port=8080 --host=localhost --open=/index.html",
    "legacy:debug": "live-server --port=8080 --host=localhost --open=/index.html --verbose"
  },
  "keywords": [
    "diamants",
    "ez-tree",
    "three.js",
    "procedural",
    "trees"
  ],
  "author": "DIAMANTS Team",
  "license": "MIT",
  "dependencies": {
    "@dgreenheck/ez-tree": "^1.0.0",
    "ray-aabb": "^3.0.2",
    "three": "^0.167.x"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.0",
    "browser-sync": "^3.0.4",
    "concurrently": "^9.2.1",
    "get-port": "^7.1.0",
    "live-server": "^1.2.2",
    "nodemon": "^3.1.10",
    "source-map-explorer": "^2.5.3",
    "vite": "^4.5.3",
    "vitest": "^1.6.0",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
